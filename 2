
 @Test
    public void testMethod1() {
        String url = "http://example.com/api/endpoint1";
        String expectedResponse = "Response from endpoint1";

        when(environment.getProperty("myapi.endpoint1.url")).thenReturn(url);
        when(restTemplate.exchange(eq(url), eq(HttpMethod.GET), any(HttpEntity.class), eq(String.class)))
                .thenReturn(ResponseEntity.ok(expectedResponse));

        String response = myRestClient.method1();

        verify(restTemplate).exchange(eq(url), eq(HttpMethod.GET), any(HttpEntity.class), eq(String.class));
        verify(environment).getProperty("myapi.endpoint1.url");
        // Additional assertions on the response can be added here
    }
